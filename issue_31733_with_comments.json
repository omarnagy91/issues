                "avatar_url": "https://avatars.githubusercontent.com/u/2112202?v=4",
                "gravatar_id": "",
                "url": "https://api.github.com/users/erquhart",
                "html_url": "https://github.com/erquhart",
                "followers_url": "https://api.github.com/users/erquhart/followers",
                "following_url": "https://api.github.com/users/erquhart/following{/other_user}",
                "gists_url": "https://api.github.com/users/erquhart/gists{/gist_id}",
                "starred_url": "https://api.github.com/users/erquhart/starred{/owner}{/repo}",
                "subscriptions_url": "https://api.github.com/users/erquhart/subscriptions",
                "organizations_url": "https://api.github.com/users/erquhart/orgs",
                "repos_url": "https://api.github.com/users/erquhart/repos",
                "events_url": "https://api.github.com/users/erquhart/events{/privacy}",
                "received_events_url": "https://api.github.com/users/erquhart/received_events",
